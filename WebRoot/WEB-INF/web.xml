<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  
  <servlet>
  <description>登陆</description>
  <display-name>Login</display-name>
  <servlet-name>Login</servlet-name>
  <servlet-class>servlet.Login</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Login</servlet-name>
  <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <description>注册</description>
  <display-name>SignUp</display-name>
  <servlet-name>SignUp</servlet-name>
  <servlet-class>servlet.SignUp</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>SignUp</servlet-name>
  <url-pattern>/SignUp</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <description>表格处理</description>
  <display-name>TableHandler</display-name>
  <servlet-name>TableHandler</servlet-name>
  <servlet-class>servlet.TableHandler</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>TableHandler</servlet-name>
  <url-pattern>/TableHandler</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <description>更新处理</description>
  <display-name>UpdateHandler</display-name>
  <servlet-name>UpdateHandler</servlet-name>
  <servlet-class>servlet.UpdateHandler</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>UpdateHandler</servlet-name>
  <url-pattern>/UpdateHandler</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>/jsp/login.jsp</welcome-file>
  </welcome-file-list>
</web-app>
